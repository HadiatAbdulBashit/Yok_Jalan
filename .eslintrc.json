{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "linebreak-style": 0,
        "no-console": 0,
        "import/no-unresolved": 0,
        "no-underscore-dangle": 0,
        "import/prefer-default-export": 0,
        "arrow-parens": 0,
        "import/no-extraneous-dependencies": 0,
        "prefer-promise-reject-errors": 0,
        "max-len": 0,
        "consistent-return": 0,
        "no-plusplus": 0,
        "no-inner-declarations": 0,
        "no-undef": 0,
        "no-shadow": 0,
        "no-empty-function": 0
    }
}
